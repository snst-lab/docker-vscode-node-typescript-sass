{
    "name": "docker-vscode-node-typescript-sass",
    "dockerComposeFile": [
        "docker-compose.yml",
        "docker-compose.extend.yml"
    ],
    "service": "app",
    "appPort": 3000,
    "extensions": [
        "VisualStudioExptTeam.vscodeintellicode",
        "dbaeumer.vscode-eslint",
        "eg2.tslint"
    ],
    "settings": {
        "terminal.integrated.shell.linux": "/bin/bash"
    },
    "shutdownAction": "none" 
}